{
  "extends": ["eslint:recommended"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": [
    "babel",
    "import"

  ],
  "globals": {
    "__DEV__": true
  },
  "settings": {
    "ecmascript": 6,
    "jsx": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "restParams": true,
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "import/no-duplicates": 2,
    "comma-dangle": 0,
    "id-length": 0,
    "no-unused-vars": [2, {"args": "none"}],
    "new-cap": 0,
    "eol-last": 0,
    "jsx-quotes": 2,
    "consistent-return": 0,
    "key-spacing": 0,
    "no-multi-spaces": 0,
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "arrow-body-style": 0,
    "array-bracket-spacing": ["warn", "always"],
    "max-len": [ "warn", { "code": 120 }],
    "semi": 2,
    "computed-property-spacing": 2,
    "radix": 0,
    "import/prefer-default-export": 0,
    "no-duplicate-imports": 0,
    "no-plusplus": 0,
    "no-continue": 0,
    "linebreak-style": ["error", "unix"]
  }
}
